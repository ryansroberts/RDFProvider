[@Burnett_1992]

[@Burnett_1993]

[@Unkown_date]

[@Comino_2013]

[@Howitt_1993]

[@Kelly_1998]

[@Taylor_2010]

[@Edelman_2001]

[@Kopelman_1995]

[@Vaughan_1996]
""";
